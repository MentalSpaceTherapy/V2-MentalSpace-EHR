{
  "name": "mental-health-dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "node deploy.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "init-db": "tsx server/init-db.ts",
    "test": "jest --detectOpenHandles",
    "test:ci": "jest --ci --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest server/ --detectOpenHandles",
    "test:backend:ci": "jest server/ --ci --detectOpenHandles --forceExit",
    "test:frontend": "vitest --environment jsdom",
    "test:frontend:ci": "vitest --environment jsdom run",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:e2e:ci": "jest --config=jest.e2e.config.js --ci --forceExit",
    "test:unit": "jest --testPathIgnorePatterns=.*\\.e2e\\.test\\.ts$ --testPathIgnorePatterns=.*\\.integration\\.test\\.ts$",
    "test:integration": "jest --testMatch=**/*.integration.test.ts",
    "test:types": "tsc --noEmit",
    "test:related": "node scripts/run-related-tests.js",
    "generate-demo-logs": "tsx server/scripts/generateDemoLogs.ts",
    "db:migrate": "tsx server/db/migrate.ts up",
    "db:migrate:create": "tsx server/db/migrate.ts create",
    "db:migrate:status": "tsx server/db/migrate.ts status",
    "db:seed": "tsx server/db/seed.ts seed",
    "db:init-migration": "tsx server/db/seed.ts init-migration",
    "db:reset": "npm run init-db && npm run db:migrate && npm run db:seed",
    "db:demo": "tsx server/db/run-demo.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
